net: "audio_train_test.prototxt"
test_iter: 300
test_state: { stage: 'test-on-test' }
test_interval: 100
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 2000
display: 20
max_iter: 3000
momentum: 0.9
weight_decay: 0.005
snapshot: 100
snapshot_prefix: "models/snapshots_lstm_audio"
solver_mode: GPU
device_id: 0
random_seed: 1701
average_loss: 1000
clip_gradients: 5
